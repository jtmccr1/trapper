{
  "name": "trapper",
  "productName": "Trapper",
  "version": "0.1.0",
  "private": true,
  "main": "public/electron.js",
  "homepage": "./",
  "dependencies": {
    "cors": "^2.8.4",
    "d3-fetch": "^1.1.2",
    "d3v4": "^4.2.2",
    "electron-is-dev": "^1.0.1",
    "glamor": "^2.20.40",
    "react": "^16.5.2",
    "react-burger-menu": "^2.6.5",
    "react-dom": "^16.5.2",
    "react-scripts": "^2.1.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron-dev": "concurrently \"BROWSER=none yarn start\" \"wait-on http://localhost:3000 && electron .\"",
    "electron-pack": "build --mac -c.extraMetadata.main=build/electron.js ",
    "preelectron-pack": "yarn build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "concurrently": "^4.1.0",
    "electron": "^4.0.5",
    "electron-builder": "^20.38.5",
    "wait-on": "^3.2.0"
  },
  "build": {
    "appId": "jt",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  }
}
